{
  "name": "@aicacia/engine",
  "version": "0.1.0",
  "description": "an entity component engine",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/aicacia/libs/ts-engine.git"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "ts-node node_modules/.bin/tape src/**/*.test.{ts,tsx} | tap-spec",
    "coverage": "nyc --reporter=lcov --require ts-node/register tape src/**/*.test.{ts,tsx} | tap-spec; nyc report ---reporter=text",
    "clean": "rm -rf ./lib",
    "format": "prettier-tslint fix '**/*.{ts,tsx}' && prettier --write '**/*.{js,jsx,json}' --ignore-path .gitignore",
    "doc": "typedoc",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' -e 'src/**/*.test.*'",
    "simple": "parcel examples/simple/index.html"
  },
  "keywords": [
    "engine",
    "scene",
    "graph"
  ],
  "author": "Nathan Faucett",
  "license": "(MIT OR Apache-2.0)",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@types/raf": "^3.4.0",
    "@types/tape": "^4.13.0",
    "minimist": "^1.2.5",
    "nyc": "^15.1.0",
    "parcel": "^1.12.4",
    "prettier": "^2.0.5",
    "prettier-tslint": "^0.4.2",
    "source-map-support": "^0.5.19",
    "tap-nyc": "^1.0.3",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@aicacia/core": "^0.1.5",
    "@aicacia/json": "^0.1.2",
    "gl-matrix": "^3.3.0",
    "raf": "^3.4.1"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "src/**/*.test.*"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ]
  }
}
