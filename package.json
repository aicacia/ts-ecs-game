{
  "name": "@aicacia/engine",
  "version": "0.1.0",
  "description": "engine",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/aicacia/libs/ts-engine.git"
  },
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "module": "./lib/index.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "ts-node node_modules/.bin/tape lib/**/*.test.{ts,tsx} | tap-spec",
    "coverage": "nyc --reporter=lcov --require ts-node/register tape lib/**/*.test.{ts,tsx} | tap-spec; nyc report ---reporter=text",
    "clean": "rm -rf ./src",
    "format": "prettier-tslint fix '**/*.{ts,tsx}' && prettier --write '**/*.{js,jsx,json}' --ignore-path .gitignore",
    "lint": "tslint -c tslint.json 'lib/**/*.{ts,tsx}' -e 'lib/**/*.test.*'",
    "simple": "parcel examples/simple/index.html",
    "collisions": "parcel examples/collisions/index.html"
  },
  "keywords": [
    "engine",
    "scene",
    "graph"
  ],
  "author": "Nathan Faucett",
  "license": "(MIT OR Apache-2.0)",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@types/gl-matrix": "^2.4.5",
    "@types/raf": "^3.4.0",
    "@types/tape": "^4.2.33",
    "nyc": "^14.1.1",
    "parcel": "^1.12.4",
    "prettier": "^1.19.1",
    "prettier-tslint": "^0.4.2",
    "source-map-support": "^0.5.16",
    "tap-nyc": "^1.0.3",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@aicacia/core": "^0.1.2",
    "@aicacia/json": "^0.1.1",
    "gl-matrix": "^3.1.0",
    "raf": "^3.4.1"
  },
  "nyc": {
    "include": [
      "lib/**/*"
    ],
    "exclude": [
      "**/*.test.*"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ]
  }
}
